/external_sd	vfat	/dev/block/mmcblk1p1								flags=display="External_sd";storage;wipeingui;removable
/internal_sd	vfat	/dev/block/mmcblk0p23								flags=display="Internal_sd";storage;wipeingui;removable

/system			ext4	/dev/block/mmcblk0p17
/data			ext4	/dev/block/mmcblk0p23 length=-16384
/cache			ext4	/dev/block/mmcblk0p20
/misc			emmc	/dev/block/mmcblk0p22
/boot			emmc	/dev/block/mmcblk0p16
/recovery		emmc	/dev/block/mmcblk0p21